/* <dtml-with base_properties> *//* <dtml-call "REQUEST.set('portal_url', portal_url())">*/.PloneRadio_portlet_streamname{}.PloneRadio_portlet_playerselect {}.PloneRadio_portlet_current_head {  font-weight: bold;  line-height: 1.5em;  background-color: darkgray;  color: white;}.PloneRadio_portlet_current {}.PloneRadio_portlet_recent_head {  font-weight: bold;  margin-top: 5px;  line-height: 1.5em;  background-color: darkgray;  color: white;}.PloneRadio_portlet_recent {  border-bottom: 1px dotted darkgray;}.PloneRadio_portlet_artist {  text-align: center;}.PloneRadio_portlet_title {  text-align: center;}.PloneRadio_portlet_listeners {  text-align: center;}.PloneRadio_portlet_album {    width: 100px;    margin: 0px auto 0px auto;}.PloneRadio_portlet_album img {  width: 100px;}/* overrides for the player-info frame */.PloneRadio_playerinfo{}.PloneRadio_playerinfo_album{  float: left;}.PloneRadio_playerinfo_artist {  text-align: left;}.PloneRadio_playerinfo_title {  text-align: left;}.PloneRadio_playerinfo_listeners {  text-align: left;}/****** SOTF Station ************/.ploneradio_sotf_station_info {  margin-bottom: 10px;}.ploneradio_sotf_station_title {}.ploneradio_sotf_station_title .link-external {  background: none;  padding: 0px;}.ploneradio_sotf_station_description {}.ploneradio_sotf_station_programme {  padding-top: 5px;  padding-bottom: 5px;  border-bottom: 1px dashed &dtml-globalBorderColor;;  clear: both;}.ploneradio_sotf_station_programme .link-external {  background: none;  padding: 0px;}.ploneradio_stof_station_programme_odd {  background-color: &dtml-oddRowBackgroundColor;;}.ploneradio_stof_station_programme_even {  background-color: &dtml-evenRowBackgroundColor;;}.ploneradio_sotf_station_programme_info {  float: left;}.ploneradio_sotf_station_entry_episode_sequence {  float: left;  margin-right: 5px;  text-align: right;  width: 30px;}.ploneradio_sotf_station_entry_title {  margin-left: 35px;  text-indent: 0px;}.ploneradio_sotf_station_entry_description {  margin-left: 35px;}.ploneradio_sotf_station_entry_broadcast_date {  margin-left: 35px;}.ploneradio_sotf_station_programme_files {  float: right;  border: 0px;}.ploneradio_sotf_clearboth {  clear: both;}/* </dtml-with> */