<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US" lang="en-US"      xmlns:metal="http://xml.zope.org/namespaces/metal"      xmlns:tal="http://xml.zope.org/namespaces/tal"      xmlns:i18n="http://xml.zope.org/namespaces/i18n"      metal:use-macro="here/main_template/macros/master"      i18n:domain="PloneRadio"><body><metal:css fill-slot="css_slot">    <link rel="stylesheet" type="text/css" media="screen" href="" tal:attributes="href string:PloneRadio.css" /></metal:css><div metal:fill-slot="main" tal:define="request python: context.portal_ploneradiotool.sotf_rss_station(context.url)">  <tal:block tal:define="station python:request[0]" tal:condition="python: test(request[1] != 1)">    <div class="ploneradio_sotf_station_info">        <div class="ploneradio_sotf_station_title"><a href="" tal:attributes="href python:station['station_url']"><h1 tal:content="python: station['station_title']">station title</h1></a></div>        <div class="ploneradio_sotf_station_description" tal:content="python: station['station_description']">station description</div>    </div>    <tal:block tal:repeat="entries python:station['entries']">        <div class="" tal:attributes="class python:test(repeat['entries'].odd(), 'ploneradio_sotf_station_programme ploneradio_stof_station_programme_odd', 'ploneradio_sotf_station_programme ploneradio_stof_station_programme_even')">            <div class="ploneradio_sotf_station_programme_info">                <div class="ploneradio_sotf_station_entry_episode_sequence" tal:content="python:entries['episode_sequence']+'.'">#</div>                <div class="ploneradio_sotf_station_entry_title"><a href="" tal:attributes="href python:entries['programme_link']" tal:content="python:entries['title']">programme title</a></div>                <div class="ploneradio_sotf_station_entry_description" tal:content="python:entries['programme_description']" tal:condition="python:test(entries['programme_description'] != '')">programme description</div>                <div class="ploneradio_sotf_station_entry_broadcast_date" tal:condition="python:test(entries['broadcast_date'] != '')"><span i18n:translate="text_sotfbroadcastdate" tal:omit-tag="">broadcasted</span> <span tal:content="python:entries['broadcast_date']" tal:omit-tag="">broadcast date</span></div>            </div>            <div class="ploneradio_sotf_station_programme_files" tal:define="global lastbitrate string:'0'">            <!-- sort files by type (stream or download)                    <tal:block tal:repeat="mediafile python:entries['mediafiles']">                  <tal:block tal:condition="python: test(mediafile['type'] == 'audiostream')">                    <a href="" tal:attributes="href python:mediafile['url']"><img src="" tal:attributes="src python:mediafile['image']" /></a><span tal:content="python:mediafile['bitrate'] + 'k ' + mediafile['format']" tal:condition="python:test((mediafile['bitrate'] != '') and (lastbitrate == mediafile['bitrate']))">bitrate</span>                  </tal:block>                 </tal:block>                <tal:block tal:repeat="mediafile python:entries['mediafiles']">                  <tal:block tal:condition="python: test(mediafile['type'] == 'audiofile')">                    <a href="" tal:attributes="href python:mediafile['url']"><img src="" tal:attributes="src python:mediafile['image']" /></a><span tal:content="python:mediafile['bitrate'] + 'k ' + mediafile['format']" tal:condition="python:test((mediafile['bitrate'] != '') and (lastbitrate == mediafile['bitrate']))">bitrate</span>                  </tal:block>                 </tal:block> -->                <tal:block tal:repeat="mediafile python:entries['mediafiles']">                    <a href="" tal:attributes="href python:mediafile['url']"><img src="" tal:attributes="src python:mediafile['image']" /></a><span tal:content="python:mediafile['bitrate'] + 'k ' + mediafile['format']" tal:condition="python:test((mediafile['bitrate'] != '') and (lastbitrate == mediafile['bitrate']))">bitrate</span>                    <span tal:define="global lastbitrate python:mediafile['bitrate']" tal:omit-tag=""></span>                </tal:block>            </div>            <div class="ploneradio_sotf_clearboth"></div>        </div>    </tal:block>  </tal:block>  <tal:block tal:condition="python: test(request[1] == 1)">    <h1>Error parsing RSS feed</h1>    <p>Most likely the server of the RSS feed could not be reached.</p>  </tal:block></div></body></html>