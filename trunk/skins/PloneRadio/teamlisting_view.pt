<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US"      lang="en-US"      metal:use-macro="here/main_template/macros/master"      i18n:domain="plone"><body><div metal:fill-slot="main"    tal:define="groups python:here.portal_groups.searchForGroups('')">    <h1 class="documentFirstHeading" tal:content="here/title">Title</h1>        <tal:block tal:repeat="group groups">        <tal:block tal:condition="python:test(group.getGroupName() not in context.ignore_groups)">            <h2 tal:content="python:group.getProperty('title', None)">Group</h2>            <metal:block tal:repeat="user python:group.getGroupMembers()">                <div class="card" tal:define="name user/getUserName;                                                fullname python: user.getProperty('fullname', None);                                                home python:container.portal_membership.getHomeUrl(name, verifyPermission=1);                                                portrait python: here.portal_membership.getPersonalPortrait(name);                                                role python: user.getProperty('ploneradio_role', None);">                    <a href="#" tal:attributes="href home">                       <img src="defaultUser.gif"                            alt=""                            border="0"                            width="75"                            height="100"                            tal:attributes="src portrait/absolute_url" />                        <br />                        <span tal:content="fullname"> member id</span><br/>                        <span style="font-size: 80%;">(<span tal:content="name" tal:omit-tag=""> member id</span>)</span><br/>                        <span tal:content="role"> Role </span>                    </a>                </div>            </metal:block>            <div class="visualClear"></div>        </tal:block>    </tal:block></div></body></html>